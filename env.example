# API Configuration
API_URL=https://api.vetfield.com
API_TIMEOUT=10000

# Environment
ENV=development

# Google Maps
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Authentication
AUTH_TOKEN_KEY=vetfield_auth_token
REFRESH_TOKEN_KEY=vetfield_refresh_token

# Payment Gateway (Stripe/Asaas)
PAYMENT_API_KEY=your_payment_api_key
PAYMENT_PUBLIC_KEY=your_payment_public_key

# Sentry (Error Tracking)
SENTRY_DSN=your_sentry_dsn

# Firebase (Optional - for real-time features)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id

# App Configuration
APP_NAME=VetField
APP_VERSION=1.0.0
